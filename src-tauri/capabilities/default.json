{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-destroy",
    "core:window:allow-set-title",
    "opener:default",
    "dialog:default",
    "fs:default",
    "fs:allow-write-text-file",
    "process:default",
    "opener:allow-default-urls"
  ]
}
